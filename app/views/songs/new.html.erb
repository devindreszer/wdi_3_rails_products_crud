<h3>New Song<h3>
  <%= form_for @song do |f| %>
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
    <br>
    <%= f.label :artist %> <br>
    <%= f.text_area :artist %>
    <br>
    <%= f.label :duration %> <br>
    <%= f.text_area :duration %>
    <br>
    <%= f.label :price %> <br>
    <%= f.text_field :price %>
    <br>
    <%= f.submit %>
  <% end %>
